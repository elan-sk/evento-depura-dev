{
  // Place your snippets for javascript here. Each snippet is defined under a snippet name and has a prefix, body and
  // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
  // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
  // same ids are connected.
  // Example:
  // "Print to console": {
  // 	"prefix": "log",
  // 	"body": [
  // 		"console.log('$1');",
  // 		"$2"
  // 	],
  // 	"description": "Log output to console"
  // }

  "Print to Jquery selector": {
    "prefix": "jqs",
    "body": ["$($1)"],
    "description": "Log output to console"
  },

  "Print to Jquery this": {
    "prefix": "jqt",
    "body": ["$(this)$1"],
    "description": "Log output to console"
  },


  "Print to console": {
    "prefix": "con",
    "body": ["console.log($1);"],
    "description": "Log output to console"
  },

  "Print to console prueba Elemento": {
    "prefix": "cone",
    "body": [
      "let $$ele = $2",
      "console.log(`$1 valor: ${$$ele}  type: ${typeof $$ele}`);",
      "console.log($$ele);",
    ],
    "description": "Log output to console"
  },

  "Print to console variable": {
    "prefix": "conv",
    "body": ["console.log(`${$1}`);"],
    "description": "Log output to console"
  },

  "Print to console variable 2": {
    "prefix": "conv2",
    "body": ["console.log(`${$1} ${$2}`);"],
    "description": "Log output to console"
  },

  "Print to console variable 3": {
    "prefix": "conv3",
    "body": ["console.log(`${$1} ${$2} ${$3}`);"],
    "description": "Log output to console"
  },

  "Print to console variable 4": {
    "prefix": "conv4",
    "body": ["console.log(`${$1} ${$2} ${$3} ${$4}`);"],
    "description": "Log output to console"
  },

  "Print to console variable 5": {
    "prefix": "conv5",
    "body": ["console.log(`${$1} ${$2} ${$3} ${$4} ${$5}`);"],
    "description": "Log output to console"
  },


  "Print to text intercalado": {
    "prefix": "menv",
    "body": ["`${$1}`;"],
    "description": "men con variable"
  },

  "Print to text intercalado 2": {
    "prefix": "menv2",
    "body": ["`${$1} ${$2}`;"],
    "description": "men con variable"
  },

  "Print to text intercalado 3": {
    "prefix": "menv3",
    "body": ["`${$1} ${$2} ${$3}`;"],
    "description": "men con variable"
  },

  "Print to text intercalado 4": {
    "prefix": "menv4",
    "body": ["`${$1} ${$2} ${$3} ${$4}`;"],
    "description": "men con variable"
  },

  "Print to text intercalado 5": {
    "prefix": "menv5",
    "body": ["`${$1} ${$2} ${$3} ${$4} ${$5}`;"],
    "description": "men con variable"
  },

  "Print to text men": {
    "prefix": "men",
    "body": ["``"],
    "description": "men"
  },

  "Print to alerta": {
    "prefix": "ale",
    "body": ["alert($1);"],
    "description": "Log output to console"
  }
}
