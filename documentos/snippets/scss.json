	"scss extend": {
		"prefix": "ex",
		"body": ["@extend %${1};${2}"],
	},

	"scss include from": {
		"prefix": "from",
		"body": [
			"@include from(${1}){${2}",
			"}"
		],
	},

	"scss include centrar vertical ": {
		"prefix": "cenv",
		"body": ["@include center('vertical');"],
	},

	"scss include centrar horizontal ": {
		"prefix": "cenh",
		"body": ["@include center('horizontal');"],
	},

	"scss include centrar both ": {
		"prefix": "cenb",
		"body": ["@include center('both');"],
	},

	"scss include bgcover ": {
		"prefix": "bgcover",
		"body": ["@include bgcover;"],
	},

	"scss include imageBg ": {
		"prefix": "imgBg",
		"body": ["@include imageBg('../img/${1}', rem($35px), rem($35px));"],
	},

	"scss include imageBgSplit ": {
		"prefix": "imgBgS",
		"body": ["@include imageBg($$path, $$btn-height, $$btn-width, ${1}, ${2});"],
	},

	"scss include background-img  ": {
		"prefix": "bg-img ",
		"body": ["background-image: url(\"../img/${1}\");"],
	},

	"scss include rem ": {
		"prefix": "rm",
		"body": ["rem(${1}px)"],
	},

	"scss include space ": {
		"prefix": "sp",
		"body": ["space(${1})"],
	},

	"scss include fluid-aspect": {
		"prefix": "asp",
		"body": ["@include fluid-aspect(${1}, 'img');"],
	},

	"scss depura responsive": {
		"prefix": "res",
		"body": [
			"\/\/prueba responsive----------------",
			"section, h1, .row {",
			"\tborder: 5px dashed red;",
			"}",
			"img {",
			"\tborder: 3px dashed yellowgreen;",
			"}",
			".container {",
			"\tborder: 3px dashed blue;",
			"\t.container {",
			"\t\tborder: 5px dashed red;",
			"\t}",
			"}",
			".row{",
			"&.g-0 {",
			"\t\tborder: 5px dashed yellow;",
			"\t}",
			"\tdiv[class*=\"col\"] {",
			"\t\tborder: 3px dashed green;",
			"\t}",
			"}",
			"border: 3px solid blue; \/\/ < 576px",
			"position: relative;",
			"&::before{",
			"\tbackground-color: blue;",
			"\tborder-radius: 0 0 0 10px;",
			"\tcontent: \"from(xs)\";",
			"\tcolor:white;",
			"\tdisplay: block;",
			"\tfont-weight: bold;",
			"\tposition: absolute;",
			"\tpadding: 3px 7px;",
			"\tright: 0;",
			"\ttop: 0;",
			"}",
			"",
			"@include from(s) { \/\/ >'s': 576px",
			"\tborder: 3px solid red;",
			"\t&::before{",
			"\t\tbackground-color: red;",
			"\t\tcontent: \"from(s)\";",
			"\t}",
			"}",
			"@include from(m) { \/\/ >'m': 768px",
			"\tborder: 3px solid yellow;",
			"\t&::before{",
			"\t\tbackground-color: yellow;",
			"\t\tcontent: \"from(m)\";",
			"\t\tcolor: black;",
			"\t}",
			"}",
			"@include from(l) { \/\/ >'l': 992px",
			"\tborder: 3px solid purple;",
			"\t&::before{",
			"\t\tbackground-color: purple;",
			"\t\tcontent: \"from(l)\";",
			"\t\tcolor: white;",
			"\t}",
			"}",
			"@include from(xl) { \/\/ >'xl': 1470px",
			"\tborder: 3px solid green;",
			"\t&::before{",
			"\t\tbackground-color: green;",
			"\t\tcontent: \"from(xl)\";",
			"\t}",
			"}",
			"\/\/?---------------------------------",
			"",
		],
		"description": "Para pruebas de responsive"
	},

